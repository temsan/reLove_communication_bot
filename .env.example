# Telegram Bot Token (REQUIRED)
BOT_TOKEN=

# Webhook settings (REQUIRED for webhook mode)
WEBHOOK_HOST=
WEBHOOK_PATH=/webhook # Можно оставить по умолчанию
WEBHOOK_SECRET="your_secret_string" # Обязательно сгенерируйте надежный секрет!

# Web server settings
WEB_SERVER_HOST=0.0.0.0 # Для запуска внутри контейнера
WEB_SERVER_PORT=8080

# Database settings (Example for PostgreSQL, adjust as needed)
# DB_TYPE=postgres
# DB_HOST=localhost
# DB_PORT=5432
# DB_USER=user
# DB_PASSWORD=password
# DB_NAME=relove_db

# Logging level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO

# Admin User IDs (comma-separated)
ADMIN_IDS= 